<script>
      const socket = io();
        let textarea = document.querySelector('#text_area')
        let msgarea = document.querySelector('.msg_area')

let n=document.getElementById('username').innerHTML;
// do{
//      n = prompt('please enter your name')

// }while(!n)

textarea.addEventListener('keyup',(e)=>{
 if(e.key==='Enter'){
     message(e.target.value);
    
 }
})

  function message(masg){

    let msg = {
    user:n,
    message: masg.trim()
    }

    appendmsg(msg,'outgoin-msg')
    /// SEND TO SERVER
    socket.emit('message',msg)
  }
  
  function appendmsg(msg,type){
  let maindiv = document.createElement('div')
  let className =type

  maindiv.classList.add(className)

  let markup =`
   <h4>${msg.user}</h4>
   <p>${msg.message}</p>
   `
 maindiv.innerHTML = markup
 msgarea.appendChild(maindiv)
  }

socket.on('message',(msg)=>{
  appendmsg(msg,'income-msg')
})
</script>
