    }

    .mobile-none.topnev-right ul {
      width: 100%;
      justify-items: space-between;
      list-style-type: none;
      flex-direction: column;
      color: white;
    }

    .mobile-none.topnev-right ul li {
      width: 100%;
      text-align: center;
      height: 30px;
    }

    .mobile-none.topnev-right ul li a {
      width: 100%;
      text-align: center;
    }

    .mobile-none.topnev-right ul li a:hover {
      width: 100%;
      text-align: center;
      background-color: rgb(74, 72, 72);
      color: orange;
    }
  }
