<section class="month-eventmain" id="events">
<div class="container">
<div class="row">
<div class="col-md-5">
  <div class="eventleft-img aos-init aos-animate" data-aos="fade-right" data-aos-duration="2000" data-aos-once="true">
      <img src="https://www.thedanceworx.com/images/girl.png" alt="Online Dance Acedemay of Danceworx">
  </div>
</div>
